	<restrictionTemplate>
		<name>Applications</name>
		<condition>// #Parameter(1) - Reference Field

select value ( Catalog.Applications.EmptyRef )
union all
select #Parameter(1)
where &amp;ApplicationsAccess = value ( Enum.Access.Undefined )
union all
select #Parameter(1)
where
#if "#Parameter(1)" = "Ref" #then
	IsFolder
#elseif "#Parameter(1)" = "Owner" #then
	cast ( #Parameter(1) as Catalog.Applications ).IsFolder
#else
	#Parameter(1).IsFolder
#endif
union all
select #Parameter(1)
where &amp;ApplicationsAccess = value ( Enum.Access.Allow )
and #Parameter(1) in ( &amp;ApplicationsList )
union all
select #Parameter(1)
where &amp;ApplicationsAccess = value ( Enum.Access.Forbid )
and #Parameter(1) not in ( &amp;ApplicationsList )
</condition>
	</restrictionTemplate>
	<restrictionTemplate>
		<name>Owners</name>
		<condition>T where T.Owner = &amp;User</condition>
	</restrictionTemplate>
	<restrictionTemplate>
		<name>Sessions</name>
		<condition>T where T.Session.User = &amp;User</condition>
	</restrictionTemplate>
	<restrictionTemplate>
		<name>SpecialAccess</name>
		<condition>1 in (
	select 1
	where not
	#if "#Parameter(1)" = "Ref" #then
		Access
	#else
		#Parameter(1).Access
	#endif
	union all
	select 1
	where &amp;User in ( select User from Catalog.Scenarios.Users where Ref = T.#Parameter(1) )
)
</condition>
	</restrictionTemplate>
	<restrictionTemplate>
		<name>Users</name>
		<condition>T where T.User = &amp;User</condition>
	</restrictionTemplate>
