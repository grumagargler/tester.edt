    </notDefaultUseAlwaysAttributes>
    <main>true</main>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>select Users.Ref, Users.DeletionMark, Users.Parent, Users.IsFolder, Users.Code, Users.Description,
	Users.AccessDenied, Users.Agent, Users.ApplicationsAccess, Users.Email,
	Users.FirstName, Users.FullName, Users.Language, Users.LastName, Users.TimeZone,
	Users.Predefined, Users.PredefinedDataName, AgentStatuses.Session.Computer as Computer,
	isnull ( AgentStatuses.Status, value ( Enum.AgentStatuses.Offline ) ) as Status	
from Catalog.Users as Users
	//
	// AgentStatuses
	//
	left join InformationRegister.AgentStatuses as AgentStatuses
	on AgentStatuses.Session.User = Users.Ref</queryText>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
      <getInvisibleFieldPresentations>true</getInvisibleFieldPresentations>
